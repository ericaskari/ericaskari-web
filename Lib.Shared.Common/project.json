{
  "projectType": "library",
  "root": "Lib.Shared.Common",
  "sourceRoot": "Lib.Shared.Common/src",
  "prefix": "app",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/Lib.Shared.Common"
      ],
      "options": {
        "jestConfig": "Lib.Shared.Common/jest.config.js",
        "passWithNoTests": true
      }
