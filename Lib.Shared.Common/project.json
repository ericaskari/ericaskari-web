    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "Lib.Shared.Common/src/**/*.ts",
          "Lib.Shared.Common/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
