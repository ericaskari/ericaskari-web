{
  "projectType": "library",
  "sourceRoot": "libs/web/common/src",
  "prefix": "app",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/web/common"
      ],
      "options": {
        "jestConfig": "libs/web/common/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/web/common/**/*.ts",
          "libs/web/common/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
